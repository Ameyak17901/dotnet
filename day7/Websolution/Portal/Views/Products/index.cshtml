@using BOL;
@{
    ViewData["Title"] = "Index Page";
}

<div>
    <h2>Product Catalog</h2>
    @{
        List<Product> allProducts= (List<Product>)ViewData["Products"];
        <ul>
            @foreach( Product product in allProducts){
                <li> <a href="/Products/details/@product.ProductId">@product.Title</a></li>
            }    
        </ul>    
    }
</div>
